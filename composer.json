{
  "name": "samfelgar/inter",
  "type": "library",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Samfelgar\\Inter\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Samfelgar\\Inter\\Tests\\": "tests/"
    }
  },
  "authors": [
    {
      "name": "Samuel Felipe",
      "email": "samfelgar@gmail.com"
    }
  ],
  "config": {
    "sort-packages": true
  },
  "require": {
    "php": "^8.2",
    "ext-mbstring": "*",
    "brick/phonenumber": "^0.6.0",
    "guzzlehttp/guzzle": "^7.8",
    "psr/http-message": "^2.0",
    "psr/log": "^3.0",
    "ramsey/uuid": "^4.7",
    "webmozart/assert": "^1.11"
  },
  "require-dev": {
    "fakerphp/faker": "^1.23",
    "phpstan/phpstan": "^1.10",
    "phpunit/phpunit": "^11.0"
  },
  "scripts": {
    "test": "phpunit",
    "analyse": "phpstan analyse"
  }
}
